{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "noImplicitAny": false
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/TherapistDashboard/EditProfileModal.jsx",
    "components/ListingsMobile.jsx",
    "components/ListingsFilterMobile.jsx",
    "components/Header.jsx",
    "components/GoogleMapsAutocomplete.jsx",
    "components/Autocomplete.jsx",
    "components/TherapistProfilePage/Scheduler/SchedulerForm.jsx",
    "components/NewUserSignUp/UserDetailsForm.jsx",
    "components/NewUserSignUp/BillingDetailsForm.jsx",
    "pages/therapists/edit-profile.jsx",
    "pages/signup.jsx",
    "pages/search.jsx",
    "pages/listings.jsx",
    "pages/api/search-for-therapists.js",
    "pages/api/endpoint-contact-therapist.js",
    "pages/account/register.jsx",
    "pages/account/login.jsx",
    "components/SearchByLocationForm.jsx",
    "components/SearchAutocomplete copy.jsx",
    "components/ReviewSlider.jsx",
    "components/PaypalButtons.jsx",
    "components/Modal.jsx",
    "components/MessageDockMobile.jsx",
    "components/LoginHeader.jsx",
    "components/LocationInput.jsx"
  ],
  "exclude": ["node_modules"]
}
